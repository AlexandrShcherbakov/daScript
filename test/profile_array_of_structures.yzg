(struct Object
    (float 3 position)
    (float 3 velocity)
)

(let
    (Object 10000 objects)
)

(defun (void update)
    (Object a)
    (
        (+= (@ (. a position) 0) (@ (. a velocity) 0))
        (+= (@ (. a position) 1) (@ (. a velocity) 1))
        (+= (@ (. a position) 2) (@ (. a velocity) 2))
    )
)

(defun (void test)
    (let
        (int index 0)
            (while (< index 10000)
                (
                    (update (@ objects index))
                    (+= index 1)
                )
            )
    )
)
