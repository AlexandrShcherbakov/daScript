(defun (void test)
    (
        (debug "hello, string\n")
        (assert true "this will not fail")
        (let
            (string a "hello")
            (string b "world")
            (
                (debug "a = ")(debug a)(debug "\n")
                (debug "b = ")(debug b)(debug "\n")
                (assert (== a a)            "string ==")
                (assert (== a "hello")      "string ==")
                (assert (!= a b)            "string !=")
                (assert (< a b)             "string <")
                (assert (<= a a)            "string <=")
                (assert (> b a)             "string >")
                (assert (>= b b)            "string >=")

            )
        )
    )
)
